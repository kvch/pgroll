{
  "name": "52_create_table_with_table_exclusion_constraint",
  "operations": [
    {
      "create_table": {
        "name": "employees",
        "columns": [
          {
            "name": "id",
            "type": "serial"
          },
          {
            "name": "start_time",
            "type": "timestamp"
          },
          {
            "name": "name",
            "type": "text"
          },
          {
            "name": "position",
            "type": "text"
          }
        ],
        "constraints": [
          {
            "name": "rooms_pk",
            "type": "primary_key",
            "columns": [
              "id"
            ]
          },
          {
            "name": "forbid_duplicated_names",
            "type": "exclude",
            "exclude": {
              "index_method": "btree",
              "elements": "name WITH =",
              "predicate": "name IS NOT NULL"
            }
          }
        ]
      }
    }
  ]
}
